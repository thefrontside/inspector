{
  "name": "@effectionx/inspector",
  "version": "0.0.1",
  "bugs": {
    "url": "https://github.com/thefrontside/inspector/issues"
  },
  "license": "MIT",
  "author": "engineering@frontside.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thefrontside/inspector.git"
  },
  "files": [
    "dist",
    "ui/dist"
  ],
  "type": "module",
  "sideEffects": true,
  "exports": {
    ".": {
      "development": "./loader.ts",
      "default": "./dist/loader.js"
    },
    "./lib": {
      "development": "./mod.ts",
      "default": "./dist/mod.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "start": "vite --host 0.0.0.0 ui",
    "build": "pnpm run build:tsc && pnpm run build:ui",
    "build:tsc": "tsc -p tsconfig.build.json",
    "build:ui": "vite build ui",
    "test": "node --test",
    "check": "pnpm run \"/(typecheck$)|(lint$)|(fmt$)/\"",
    "typecheck": "tsc -b --noEmit",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check"
  },
  "dependencies": {
    "@ark/util": "^0.56.0",
    "@b9g/crank": "^0.7.5",
    "@effectionx/stream-helpers": "^0.8.0",
    "@standard-schema/spec": "^1.0.0",
    "arktype": "^2.1.27",
    "d3": "^7.9.0",
    "h3": "2.0.1-rc.14",
    "remeda": "^2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@effectionx/bdd": "^0.4.2",
    "@nano-router/history": "^4.0.4",
    "@nano-router/router": "^4.0.4",
    "@shoelace-style/shoelace": "^2.20.1",
    "@types/d3": "^7.4.3",
    "@types/node": "22",
    "d3": "^7.9.0",
    "effection": "4.1.0-alpha.5",
    "expect": "^30.2.0",
    "lightningcss": "^1.31.1",
    "oxfmt": "^0.34.0",
    "oxlint": "^1.49.0",
    "parse-sse": "^0.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-static-copy": "^3.2.0"
  },
  "peerDependencies": {
    "effection": "^4.1.0-alpha.3"
  },
  "packageManager": "pnpm@9.15.9"
}
